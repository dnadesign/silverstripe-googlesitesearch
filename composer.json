{
	"name": "dnadesign/silverstripe-googlesitesearch",
	"description": "Add Google Site Search to your SilverStripe Site.",
    "type": "silverstripe-vendormodule",
	"keywords": ["silverstripe", "googlesitesearch", "search", "seo"],
	"homepage": "https://github.com/dnadesign/silverstripe-googlesitesearch",
	"license": "BSD-3-Clause",
	"authors": [{
		"name": "Will Rossiter",
		"email": "will.rossiter@dna.co.nz"
	}],
	"require": {
		"silverstripe/recipe-cms": "^1.0 || ^4.0",
	    "silverstripe/vendor-plugin": "^1.0"
	},
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "DNADesign\\GoogleSiteSearch\\": "src/",
            "DNADesign\\GoogleSiteSearch\\Test\\": "tests/"
        }
    },
	"extra": {
		"installer-name": "googlesitesearch",
		"expose": [
			"css",
			"images",
			"javascript"
		]
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
